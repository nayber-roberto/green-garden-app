<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginActivity">


    <View
        android:id="@+id/view_header_login"
        android:layout_width="411dp"
        android:layout_height="276dp"
        android:background="@drawable/header_view_login"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="134dp"
        android:layout_height="134dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.085"
        app:srcCompat="@drawable/logo_green_garden_white" />

    <View
        android:id="@+id/view_container_login"
        android:layout_width="310dp"
        android:layout_height="399dp"
        android:background="@drawable/container_login_rounded_view"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.688" />

    <TextView
        android:id="@+id/textView_Iniciar_Sesion"
        android:layout_width="170dp"
        android:layout_height="33dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Iniciar Sesión"
        android:textAlignment="center"
        android:textColor="#264325"
        android:textSize="22sp"
        android:textStyle="normal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.385" />

    <EditText
        android:id="@+id/editTextUsername"
        android:layout_width="270dp"
        android:layout_height="48dp"
        android:background="@drawable/edittext_input_login"
        android:fontFamily="@font/poppins_light"
        android:hint="Usuario"
        android:inputType="text"
        android:textAlignment="center"
        android:textColorHint="#D8D8D8"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_header_login"
        app:layout_constraintVertical_bias="0.478" />

    <EditText
        android:id="@+id/editTextPassword"
        android:layout_width="270dp"
        android:layout_height="48dp"
        android:background="@drawable/edittext_input_login"
        android:fontFamily="@font/poppins_light"
        android:hint="Contraseña"
        android:inputType="textPassword"
        android:textAlignment="center"
        android:textColorHint="#D8D8D8"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/view_container_login"
        app:layout_constraintEnd_toEndOf="@+id/view_container_login"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toStartOf="@+id/view_container_login"
        app:layout_constraintTop_toTopOf="@+id/view_container_login"
        app:layout_constraintVertical_bias="0.478" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="14dp"
        android:layout_height="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextUsername"
        app:layout_constraintEnd_toEndOf="@+id/editTextUsername"
        app:layout_constraintHorizontal_bias="0.082"
        app:layout_constraintStart_toStartOf="@+id/editTextUsername"
        app:layout_constraintTop_toTopOf="@+id/editTextUsername"
        app:layout_constraintVertical_bias="0.464"
        app:srcCompat="@drawable/usuario" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextPassword"
        app:layout_constraintEnd_toEndOf="@+id/editTextPassword"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="@+id/editTextPassword"
        app:layout_constraintTop_toTopOf="@+id/editTextPassword"
        app:srcCompat="@drawable/password"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_ingresar"
        android:layout_width="185dp"
        android:layout_height="44dp"
        android:backgroundTint="#264325"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:text="Ingresar"
        android:textColorHighlight="#264325"
        android:textColorLink="#264325"
        android:textSize="14sp"
        android:fontFamily="@font/poppins_semibold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_header_login"
        app:layout_constraintVertical_bias="0.663" />

    <Button
        android:id="@+id/btnRegistrarse"
        android:layout_width="185dp"
        android:layout_height="44dp"
        android:backgroundTint="#00264325"
        android:fontFamily="@font/poppins_semibold"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:text="Registrarse"
        android:textColor="#264325"
        android:textColorHighlight="#264325"
        android:textColorLink="#264325"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_header_login"
        app:layout_constraintVertical_bias="0.727" />

    <View
        android:id="@+id/divider"
        android:layout_width="250dp"
        android:layout_height="1dp"
        android:layout_marginStart="29dp"
        android:layout_marginEnd="31dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="@+id/view_container_login"
        app:layout_constraintEnd_toEndOf="@+id/view_container_login"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/view_container_login"
        app:layout_constraintTop_toTopOf="@+id/view_container_login"
        app:layout_constraintVertical_bias="0.824" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="38dp"
        android:layout_height="38dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_container_login"
        app:layout_constraintEnd_toEndOf="@+id/view_container_login"
        app:layout_constraintHorizontal_bias="0.319"
        app:layout_constraintStart_toStartOf="@+id/view_container_login"
        app:layout_constraintTop_toTopOf="@+id/view_container_login"
        app:layout_constraintVertical_bias="0.952"
        app:srcCompat="@drawable/google_logo" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="38dp"
        android:layout_height="38dp"
        app:layout_constraintBottom_toBottomOf="@+id/view_container_login"
        app:layout_constraintEnd_toEndOf="@+id/view_container_login"
        app:layout_constraintHorizontal_bias="0.672"
        app:layout_constraintStart_toStartOf="@+id/view_container_login"
        app:layout_constraintTop_toTopOf="@+id/view_container_login"
        app:layout_constraintVertical_bias="0.952"
        app:srcCompat="@drawable/facebook_logo" />

    <Button
        android:id="@+id/button_olvido_contrasena"
        android:layout_width="230dp"
        android:layout_height="35dp"
        android:backgroundTint="#00264325"
        android:fontFamily="@font/poppins_semibold"
        android:paddingTop="3dp"
        android:paddingBottom="3dp"
        android:text="¿Olvidó su contraseña?"
        android:textColor="#264325"
        android:textColorHighlight="#264325"
        android:textColorLink="#264325"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view_header_login"
        app:layout_constraintVertical_bias="0.954" />

</androidx.constraintlayout.widget.ConstraintLayout>